{
  "lerna": "2.0.0-beta.20",
  "version": "6.10.1",
  "changelog": {
    "repo": "babel/babel",
    "labels": {
      "tag: bug fix": "Bug Fix",
      "tag: breaking change": "Breaking Change",
      "tag: documentation": "Documentation",
